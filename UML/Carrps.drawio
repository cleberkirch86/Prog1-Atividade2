<mxfile>
  <diagram name="Classe Carros">
    <mxGraphModel dx="2000" dy="1200" grid="1" gridSize="10" guides="1" tooltips="1" connect="1">
      <root>
        <mxCell id="0" />
        <mxCell id="1" parent="0" />

        <!-- Classe Principal -->
        <mxCell id="2" value="Carros" style="shape=rectangle;rounded=0;whiteSpace=wrap;html=1;fontStyle=1;align=center;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="100" y="100" width="300" height="200" as="geometry" />
        </mxCell>

        <!-- Seção Atributos -->
        <mxCell id="3" value="+marca: String<br>+modelo: String<br>+ano: int" style="shape=rectangle;rounded=0;whiteSpace=wrap;html=1;align=left;spacingLeft=10;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="2">
          <mxGeometry y="30" width="300" height="70" as="geometry" />
        </mxCell>

        <!-- Linha separadora entre atributos e métodos -->
        <mxCell id="4" style="line;strokeColor=#000000;" edge="1" parent="2">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>

        <!-- Seção Métodos -->
        <mxCell id="5" value="+ligar(): void<br>+acelerar(): void<br>+frear(): void" style="shape=rectangle;rounded=0;whiteSpace=wrap;html=1;align=left;spacingLeft=10;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="2">
          <mxGeometry y="100" width="300" height="80" as="geometry" />
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
